<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Front</title>
  <%= stylesheet_link_tag    'front', media: 'all' %>
  <%= javascript_include_tag 'front' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="header">
    <%= render 'shared/header' %>
  </div>

  <div class="main col-sm-12">
    <div class="middle col-sm-9">
      <%= yield %>
    </div>
    <div class="middle col-sm-3">
      <%= render 'shared/side' %>
    </div>

  </div>

  <div class="footer">
    <%= render 'shared/footer' %>
  </div>

  <%= javascript_include_tag params[:controller] %>
</body>
</html>
