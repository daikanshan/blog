<%= form_for(@front_nav) do |f| %>
  <% if @front_nav.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@front_nav.errors.count, "error") %> prohibited this front_nav from being saved:</h2>

      <ul>
      <% @front_nav.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :text %><br>
    <%= f.text_field :text %>
  </div>
  <div class="field">
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </div>
  <div class="field">
    <%= f.label :icon %><br>
    <%= f.text_field :icon %>
  </div>
  <div class="form-group sr-only">
    <%= f.select :supnav_id,options_from_collection_for_select(@front_navs,'id','text',"#{@supnav_id}"),
                    include_blank: "选择父目录" %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
